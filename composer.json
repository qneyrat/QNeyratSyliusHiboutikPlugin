{
    "name": "qneyrat/sylius-hiboutik-plugin",
    "type": "sylius-hiboutik-plugin",
    "authors": [
        {
            "name": "qneyrat",
            "email": "quentin.neyrat@gmail.com"
        }
    ],
    "keywords": ["sylius", "sylius-plugin"],
    "description": "Hiboutik sync plugin for Sylius.",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "sylius/sylius": "1.7",
        "fig/http-message-util": "^1.1",
        "symfony/http-client": "^5.0",
        "doctrine/common": "^2.12",
        "symfony/serializer": "^5.0",
        "symfony/property-access": "^4.4",
        "symfony/property-info": "^5.0",
        "phpdocumentor/reflection-docblock": "^5.1",
        "symfony/cache": "^5.0",
        "symfony/serializer-pack": "^1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-doctrine": "^0.12.0",
        "phpstan/phpstan-strict-rules": "^0.12.0",
        "phpstan/phpstan-webmozart-assert": "^0.12.0",
        "phpunit/phpunit": "^8.0",
        "sensiolabs/security-checker": "^6.0",
        "sylius-labs/coding-standard": "^3.1",
        "symfony/browser-kit": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/intl": "^4.4",
        "symfony/panther": "^0.7.1",
        "symfony/web-profiler-bundle": "^4.4",
        "symfony/web-server-bundle": "^4.4"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "QNeyrat\\SyliusHiboutikPlugin\\": "src/",
            "Tests\\QNeyrat\\SyliusHiboutikPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/Kernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6-dev"
        }
    }
}
